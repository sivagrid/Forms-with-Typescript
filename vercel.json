{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
      { "src": "public/", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/bundle.js", "dest": "/bundle.js" },
      { "src": "/styles.css", "dest": "/styles.css" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/index.html"}
    ]
  }